{
  "exclude": ["node_modules", "**/*.spec.ts"],
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": ["es6"],
    "allowJs": false,
    "outDir": "./build",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": false,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "strictPropertyInitialization": false,
    "emitDecoratorMetadata": false
  }
}
